{
  "name": "auroraapi",
  "version": "0.0.1",
  "description": "Aurora SDK for NodeJS",
  "main": "dist/index",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "yarn lint && yarn test && yarn build",
    "precommit": "yarn lint && yarn build",
    "prepush": "yarn test",
    "build": "rm -rf dist && tsc --pretty --declaration",
    "lint": "tslint -c tslint.json -p tsconfig.json -e node_modules --fix",
    "test": "mocha -r ts-node/register \"lib/tests/**/*.ts\"",
    "test-integ": "mocha -r ts-node/register lib/tests/integration.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auroraapi/aurora-nodejs.git"
  },
  "keywords": [
    "aurora",
    "speech",
    "language",
    "text",
    "recognition",
    "api",
    "sdk"
  ],
  "author": "Nikhil Kansal <nkansal96@gmail.com>, Christopher Brown, Anbo Wei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkansal96/aurora-nodejs/issues"
  },
  "homepage": "https://github.com/nkansal96/aurora-nodejs#readme",
  "engines": {
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.4.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "https-post": "^0.1.1",
    "naudiodon": "^0.4.6",
    "stream-buffers": "^3.0.1"
  }
}
